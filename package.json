{
  "name": "craft-and-local-api-v2",
  "version": "1.0.0",
  "description": "Craft & Local API V2",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "8.9.4",
    "yarn": "1.6.0"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production",
    "prod:start": "cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && jest --coverage",
    "test:watch": "jest --watch",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "aws-sdk": "^2.315.0",
    "babel-polyfill": "^6.23.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.9.0",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.5",
    "morgan": "^1.8.2",
    "multer": "^1.3.1",
    "ramda": "^0.25.0",
    "request": "^2.87.0"
  }
}
